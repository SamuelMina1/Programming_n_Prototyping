import math 
r = int(input("What is the radius of your circle?"))
c = 2*r*math.pi 
a = math.pi*r**2
print("c=",c," a=",a)

import math 
r = float(input("What is the radius of your cylinder?"))
h = int(input("What is the hieght of your cylinder?"))
c = 2*r*math.pi 
v = math.pi*r**2*h
print("v = " +str(v))

